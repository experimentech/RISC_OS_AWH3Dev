; IDMAC CPMemory Field Words
; refers to field to be accessed in cpmem 160 bit megaword, and its width
; built as cpmem word (0|1)<<0 + startbit<<8 + field width<<16
CP_XV   *       (0<<0) + (10<<8)  + (0<<16)
CP_YV   *       (0<<0) + (9<<8)   + (10<<16)
CP_XB   *       (0<<0) + (19<<8)  + (13<<16)
CP_YB   *       (0<<0) + (32<<8)  + (12<<16)
CP_NSB_B *      (0<<0) + (44<<8)  + (1<<16)
CP_CF   *       (0<<0) + (45<<8)  + (1<<16)
;stuff for non interleaved
CPN_UBO *       (0<<0) + (46<<8)  + (22<<16)
CPN_VBO *       (0<<0) + (68<<8)  + (22<<16)
CPN_IOX *       (0<<0) + (90<<8)  + (4<<16)
CPN_RDRW *      (0<<0) + (94<<8)  + (1<<16)
;stuff for interleaved
CPI_SX  *       (0<<0) + (46<<8)  + (12<<16)
CPI_SY  *       (0<<0) + (58<<8)  + (11<<16)
CPI_NS  *       (0<<0) + (69<<8)  + (10<<16)
CPI_SDX *       (0<<0) + (79<<8)  + (7<<16)
CPI_SM  *       (0<<0) + (86<<8)  + (10<<16)
CPI_SCC *       (0<<0) + (96<<8)  + (1<<16)
CPI_SCE *       (0<<0) + (97<<8)  + (1<<16)
CPI_SDY *       (0<<0) + (98<<8)  + (7<<16)
CPI_SDRX *      (0<<0) + (105<<8) + (1<<16)
CPI_SDRY *      (0<<0) + (106<<8) + (1<<16)
CPI_BPP *       (0<<0) + (107<<8) + (3<<16)
CPI_DEC_SEL *   (0<<0) + (110<<8) + (2<<16)
CPI_DIM *       (0<<0) + (112<<8) + (1<<16)
;
CP_SO   *       (0<<0) + (113<<8) + (1<<16)
CP_BNDM *       (0<<0) + (114<<8) + (3<<16)
CP_BM   *       (0<<0) + (117<<8) + (2<<16)
CP_ROT  *       (0<<0) + (119<<8) + (1<<16)
CP_HF   *       (0<<0) + (120<<8) + (1<<16)
CP_VF   *       (0<<0) + (121<<8) + (1<<16)
CP_THE  *       (0<<0) + (122<<8) + (1<<16)
CP_CAP  *       (0<<0) + (123<<8) + (1<<16)
CP_CAE  *       (0<<0) + (124<<8) + (1<<16)
CP_FW   *       (0<<0) + (125<<8) + (13<<16)
CP_FH   *       (0<<0) + (138<<8) + (12<<16)
CP_EOLI *       (0<<0) + (150<<8) + (1<<16)
CP_EAB0 *       (1<<0) + (0<<8)   + (29<<16)
CP_EAB1 *       (1<<0) + (29<<8)  + (29<<16)
CP_ILO  *       (1<<0) + (58<<8)  + (20<<16)
CP_NPB  *       (1<<0) + (78<<8)  + (7<<16)
CP_PFS  *       (1<<0) + (85<<8)  + (4<<16)
CP_ALU  *       (1<<0) + (89<<8)  + (1<<16)
CP_ALB  *       (1<<0) + (90<<8)  + (3<<16)
CP_ID   *       (1<<0) + (93<<8)  + (2<<16)
CP_TH   *       (1<<0) + (95<<8)  + (7<<16)
; Stuff for non interleave
CPN_SLY *       (1<<0) + (102<<8) + (14<<16)
; stuff for interleave
CPI_SL *        (1<<0) + (102<<8) + (14<<16)
CPI_WID0 *      (1<<0) + (116<<8) + (3<<16)
CPI_WID1 *      (1<<0) + (119<<8) + (3<<16)
CPI_WID2 *      (1<<0) + (122<<8) + (3<<16)
;
CP_WID3 *       (1<<0) + (125<<8) + (3<<16)
; stuff for non interleave
CPN_SLUV *      (1<<0) + (128<<8) + (14<<16)
; stuff for interleave
CPI_OFS0 *      (1<<0) + (128<<8) + (5<<16)
CPI_OFS1 *      (1<<0) + (133<<8) + (5<<16)
CPI_OFS2 *      (1<<0) + (138<<8) + (5<<16)
CPI_OFS3 *      (1<<0) + (143<<8) + (5<<16)
CPI_SZYS *      (1<<0) + (148<<8) + (1<<16)
;
CP_CRE  *       (1<<0) + (149<<8 + (1<<16) )
; interleaved stuff
CPI_DEC_SEL2  * (1<<0) + (148<<8) + (1<<16)

;IDMAC config structure ipu_idmac_info_t
                ^       0,v3
ipu_idmac_info  #       0
iii_channel     #       4
iii_addr0       #       4
iii_addr1       #       4
iii_width       #       4
iii_height      #       4
iii_npb #               4
iii_pixel_format #      4
iii_sl  #               0          ; sl for interleaved modes,
iii_sly #               4          ; sly for non interleaved modes
iii_u_offset    #       4
iii_bpp #               4
iii_so  #               4
iii_ilo #               4
iii_bm  #               4
iii_rot #               4
iii_hf  #               4
iii_vf  #               4
ipu_idmac_info_size * (:INDEX:@ - :INDEX:ipu_idmac_info)

; pixel types (ips_dma_colorimetry_e)
NON_INTERLEAVED_YUV444          * &0
NON_INTERLEAVED_YUV422          * &1
NON_INTERLEAVED_YUV420          * &2
PARTIAL_INTERLEAVED_YUV422      * &3
PARTIAL_INTERLEAVED_YUV420      * &4
INTERLEAVED_LUT                 * &5
INTERLEAVED_GENERIC             * &6
