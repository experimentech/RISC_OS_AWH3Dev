	= "orecvfrom", 0
	ALIGN
	& &FF00000C
orecvfrom
	MOV	ip,sp
	STMDB	sp!,{a1-a4}
	STMDB	sp!,{v1-v2,fp,ip,lr,pc}
	SUB	fp,ip,#20
	LDMIA	ip,{v1-v2}
