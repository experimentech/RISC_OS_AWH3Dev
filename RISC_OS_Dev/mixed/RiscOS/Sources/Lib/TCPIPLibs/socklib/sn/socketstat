	GET	hdr.InetSWIs

	AREA	SockLib,CODE,READONLY

	IMPORT	_copyerror
	EXPORT	socketstat

	= "socketstat", 0
	ALIGN
	& &FF00000C
socketstat
	MOV	ip,sp
	STMDB	sp!,{a1-a2,fp,ip,lr,pc}
	SUB	fp,ip,#4
	SWI	XSocket_Stat
	BLVS	_copyerror
	[ {CONFIG}=26
	LDMDB	fp,{fp,sp,pc}^
	|
	LDMDB	fp,{fp,sp,pc}
	]

	END
