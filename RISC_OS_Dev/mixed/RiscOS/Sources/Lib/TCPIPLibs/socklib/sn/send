	GET	hdr.InetSWIs

	AREA	SockLib,CODE,READONLY

	IMPORT	_copyerror
	EXPORT	send

	= "send", 0
	ALIGN
	& &FF000008
send
	MOV	ip,sp
	STMDB	sp!,{a1-a4,fp,ip,lr,pc}
	SUB	fp,ip,#4
	SWI	XSocket_Send
	BLVS	_copyerror
	[ {CONFIG}=26
	LDMDB	fp,{fp,sp,pc}^
	|
	LDMDB	fp,{fp,sp,pc}
	]

	END
