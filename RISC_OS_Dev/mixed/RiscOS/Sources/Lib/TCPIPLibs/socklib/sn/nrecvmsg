	GET	hdr.InetSWIs

	AREA	SockLib,CODE,READONLY

	IMPORT	_copyerror
	EXPORT	nrecvmsg

	= "nrecvmsg", 0
	ALIGN
	& &FF00000C
nrecvmsg
	MOV	ip,sp
	STMDB	sp!,{a1-a3,fp,ip,lr,pc}
	SUB	fp,ip,#4
	SWI	XSocket_Recvmsg_1
	BLVS	_copyerror
	[ {CONFIG}=26
	LDMDB	fp,{fp,sp,pc}^
	|
	LDMDB	fp,{fp,sp,pc}
	]

	END
