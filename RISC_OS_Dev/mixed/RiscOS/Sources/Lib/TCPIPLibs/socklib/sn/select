	GET	hdr.InetSWIs

	AREA	SockLib,CODE,READONLY

	IMPORT	_copyerror
	EXPORT	select

	= "select", 0
	ALIGN
	& &FF000008
select
	MOV	ip,sp
	STMDB	sp!,{a1-a4}
	STMDB	sp!,{v1,fp,ip,lr,pc}
	SUB	fp,ip,#20
	LDR	v1,[ip]
	SWI	XSocket_Select
	BLVS	_copyerror
	[ {CONFIG}=26
	LDMDB	fp,{v1,fp,sp,pc}^
	|
	LDMDB	fp,{v1,fp,sp,pc}
	]

	END
