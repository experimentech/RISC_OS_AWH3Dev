	GET	hdr.InetSWIs

	AREA	SockLib,CODE,READONLY

	IMPORT	_copyerror
	EXPORT	bind

	= "bind", 0
	ALIGN
	& &FF000008
bind
	MOV	ip,sp
	STMDB	sp!,{a1-a3,fp,ip,lr,pc}
	SUB	fp,ip,#4
	SWI	XSocket_Bind
	BLVS	_copyerror
	[ {CONFIG}=26
	LDMDB	fp,{fp,sp,pc}^
	|
	LDMDB	fp,{fp,sp,pc}
	]

	END
